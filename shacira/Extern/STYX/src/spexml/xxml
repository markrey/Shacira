#! /bin/sh -e

patdir=/home/heike/styx-bin/share/examples/pat
spedir=/home/heike/styx-bin/share/examples/spe
dstdir=.

stydoc -keepNTs=Attrs:Attr -OutFile=${dstdir}/xml_cfg.htm ${spedir}/xml.sty ${patdir}/tstydoc_us.htm 
stydoc -OutFile=${dstdir}/dtd_cfg.htm ${spedir}/dtd.sty ${patdir}/tstydoc_us.htm 
